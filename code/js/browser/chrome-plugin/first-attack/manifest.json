{
    "name": "first-attack",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "48": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["http://221.204.213.204:82/*"],
            "js": ["index.js", "register.js", "login.js"]
        }
    ],
    "permissions": ["http://*.google.com/", "https://*.google.com/"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Do action",
        "default_popup": "index.html"
    }
}  